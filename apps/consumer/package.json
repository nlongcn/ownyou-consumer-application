{
  "name": "@ownyou/consumer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:pwa": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:wallet": "playwright test e2e/wallet-persistence.spec.ts",
    "test:wallet:headed": "playwright test e2e/wallet-persistence.spec.ts --headed",
    "test:api": "node --env-file=.env -e \"fetch('https://api.openai.com/v1/models', {headers: {Authorization: 'Bearer ' + process.env.VITE_OPENAI_API_KEY}}).then(r => r.ok ? console.log('✅ OpenAI API key valid') : r.json().then(e => console.log('❌ API key error:', e.error?.message || r.status)))\"",
    "lint": "eslint src --ext ts,tsx",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "@ownyou/agents-base": "workspace:*",
    "@ownyou/agents-diagnostic": "workspace:*",
    "@ownyou/agents-events": "workspace:*",
    "@ownyou/agents-restaurant": "workspace:*",
    "@ownyou/agents-shopping": "workspace:*",
    "@ownyou/agents-travel": "workspace:*",
    "@ownyou/data-calendar": "workspace:*",
    "@ownyou/data-financial": "workspace:*",
    "@ownyou/email": "workspace:*",
    "@ownyou/iab-classifier": "workspace:*",
    "@ownyou/ikigai": "workspace:*",
    "@ownyou/llm-client": "workspace:*",
    "@ownyou/memory": "workspace:*",
    "@ownyou/memory-store": "workspace:*",
    "@ownyou/reflection": "workspace:*",
    "@ownyou/resilience": "workspace:*",
    "@ownyou/shared-types": "workspace:*",
    "@ownyou/sync": "workspace:*",
    "@ownyou/triggers": "workspace:*",
    "@ownyou/ui-components": "workspace:*",
    "@ownyou/ui-design-system": "workspace:*",
    "@tanstack/react-query": "^5.60.0",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-deep-link": "^2.0.0",
    "@tauri-apps/plugin-http": "^2.5.4",
    "@tauri-apps/plugin-log": "^2.7.1",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
