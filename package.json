{
  "name": "ownyou-consumer-app",
  "version": "0.1.0",
  "private": true,
  "description": "OwnYou Consumer Application - Privacy-first personal AI with IAB classification and Mission Agents",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.11",
    "@langchain/core": "^0.3.24",
    "@langchain/langgraph": "^0.2.26",
    "@langchain/openai": "^0.3.15",
    "@steerprotocol/langgraph-checkpoint-pglite": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "fake-indexeddb": "^6.2.4",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.7.2",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
