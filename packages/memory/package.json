{
  "name": "@ownyou/memory",
  "version": "0.1.0",
  "description": "OwnYou memory intelligence - tools, embeddings, retrieval, and lifecycle",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tools": {
      "import": "./dist/tools/index.js",
      "types": "./dist/tools/index.d.ts"
    },
    "./embedding": {
      "import": "./dist/embedding/index.js",
      "types": "./dist/embedding/index.d.ts"
    },
    "./retrieval": {
      "import": "./dist/retrieval/index.js",
      "types": "./dist/retrieval/index.d.ts"
    },
    "./lifecycle": {
      "import": "./dist/lifecycle/index.js",
      "types": "./dist/lifecycle/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ownyou/shared-types": "workspace:*",
    "@ownyou/memory-store": "workspace:*"
  },
  "devDependencies": {
    "fake-indexeddb": "^6.0.0",
    "typescript": "^5.7.2",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@xenova/transformers": "^2.17.0"
  },
  "peerDependenciesMeta": {
    "@xenova/transformers": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
