{
  "name": "@ownyou/memory-store",
  "version": "0.1.0",
  "description": "OwnYou LangGraph Store-compatible memory storage with hybrid search",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./backends": "./src/backends/index.ts",
    "./search": "./src/search/index.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ownyou/shared-types": "workspace:*",
    "idb": "^8.0.0"
  },
  "devDependencies": {
    "fake-indexeddb": "^6.0.0",
    "typescript": "^5.7.2",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@xenova/transformers": "^2.17.0"
  },
  "peerDependenciesMeta": {
    "@xenova/transformers": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
