{
  "name": "@ownyou/oauth",
  "version": "0.1.0",
  "description": "Unified OAuth management for browser and desktop - Sprint 1b",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.js",
      "types": "./dist/storage/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ownyou/shared-types": "workspace:*"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.0.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@tauri-apps/api": {
      "optional": true
    }
  }
}
